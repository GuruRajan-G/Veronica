@using System.Web.Optimization
@using WebHelpers.Mvc5.Enum
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>
        @if (!string.IsNullOrWhiteSpace(ViewBag.Title))
        {
            @ViewBag.Title@: -
        }
        AdminLTE
    </title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">

    <script src="@RouteJs.RouteJsHandler.HandlerUrl"></script>
    <script src="@EnumHandler.HandlerUrl"></script>
    @Scripts.Render("~/Bundles/js")
    @RenderSection("ViewSpecificJavascript", required: false)

    @if ((ViewBag.Title == "DollarActiveJob"))
    {
        @Styles.Render("~/content/css")
        @Scripts.Render("~/bundles/jqueryui")
        @Scripts.Render("~/bundles/jqueryBlockUI")
        @Scripts.Render("~/bundles/dataTable")
        @Scripts.Render("~/bundles/bootstrap")
        @Scripts.Render("~/bundles/application")
        @Scripts.Render("~/bundles/Dollar")
        @Styles.Render("~/Bundles/css")
    }

    @if ((ViewBag.Title == "ErrorDashboard"))
    {
    @Styles.Render("~/content/css")
    @*@Scripts.Render("~/bundles/jquery")*@
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/jqueryBlockUI")
    @Scripts.Render("~/bundles/dataTable")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/application")
    @Scripts.Render("~/bundles/Errordashboard")
    @Styles.Render("~/Bundles/css")
    }
    @if ((ViewBag.Title == "SSISDashboard"))
    {
        @Styles.Render("~/content/css")
        @Scripts.Render("~/bundles/jqueryui")
        @Scripts.Render("~/bundles/jqueryBlockUI")
        @Scripts.Render("~/bundles/dataTable")
        @Scripts.Render("~/bundles/bootstrap")
        @Scripts.Render("~/bundles/application")
        @Scripts.Render("~/bundles/dashboard")
        @Styles.Render("~/Bundles/css")
    }


    <!-- global for use in scripts -->
    <script type="text/javascript">
        var currentDomain = '@System.Web.HttpContext.Current.Request.Url'
    </script>
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        @Html.Partial("_Header")
        @Html.Partial("_Sidebar")

        <div class="content-wrapper">
            <section class="content-header">
                <h1>
                    @ViewBag.Title
                    <small>@ViewBag.Description</small>
                </h1>
            </section>

            <section class="content">
                @*<div class="row">
                    <div class="col-md-12">
                        <div id="mainContainer">*@
                            @RenderBody()
                        @*</div>
                    </div>
                </div>*@
            </section>
        </div>
        @Html.Partial("_ControlSidebar")
    </div>

    
</body>
</html>